{
    "rewrites": [
        {
            "source": "/api/exams/(.*)",
            "destination": "/api/exams"
        },
        {
            "source": "/api/submissions/(.*)",
            "destination": "/api/submissions"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ]
}